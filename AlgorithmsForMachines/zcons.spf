PROC ZCONS  (REAL GRA, REAL ZZI, REAL ZZF, REAL COMP)
MSG("" << GRA  << ZZI << ZZF << COMP)

R30=$AA_IW[X];X
R40=$AA_IW[Y];Y
ATRANS X=R30 Y=R40


R11=ZZI;Z_INICIAL
R12=R2;INCREMENTO
R13=ZZF;Z_FINAL
R14=R4;S
R15=R5;F

G0 Z=R1+200
S=R4 M3
X0 Y0 ;POSICIONAMENTO
Z=R1+1
RETORNO2:
IF R11 < R13 GOTOF RETORNO1
Z=R1

AROT X=GRA
Y=IC(COMP)
AMIRROR Y0

R1=R1-R2
GOTOB RETORNO2
RETORNO1:

G0 Z=R1+250
M30

M17
